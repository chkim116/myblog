useGetPostById(`/api/${id}`, location);\r\n\r\n    const [allLoading, setAllLoading] = useState(false);\r\n\r\n    useEffect(() => {\r\n        if (allPost) {\r\n            return setAllLoading(true);\r\n        }\r\n        const getAllPost = () => {\r\n            const AllPost = async () => {\r\n                try {\r\n                    const posting = await Axios.get(\"/api/all\").then(\r\n                        (res) => res.data\r\n                    );\r\n                    dispatch(getAllPostForLength(posting));\r\n                    setAllLoading(true);\r\n                } catch (err) {\r\n                    console.log(err);\r\n                }\r\n            };\r\n            AllPost();\r\n        };\r\n        getAllPost();\r\n    }, [allPost]);\r\n\r\n    //  del post\r\n\r\n    const [del, setDel] = useState(false);\r\n\r\n    const onClick = () => {\r\n        if (window.confirm(\"정말 삭제하겠습니까?\")) {\r\n            const deletePost = async () => {\r\n                try {\r\n                    dispatch(delPostOnClick(id));\r\n                    await Axios.get(`/api/del/${id}`).then((res) =>\r\n                        setDel(res.data)\r\n                    );\r\n                } catch (err) {\r\n                    console.log(err);\r\n                    setDel(false);\r\n                }\r\n            };\r\n            deletePost();\r\n        }\r\n    };\r\n\r\n    useEffect(() => {\r\n        if (del) {\r\n            history.push(\"/post\");\r\n        }\r\n    }, [del]);\r\n\r\n    // 코멘트 작성\r\n\r\n    const [comment, setComment] = useState({ comment: \"\" });\r\n    const [fakeComment, setFakeComment] = useState([]);\r\n    const onChangeComment = (e) => {\r\n        setComment({ ...comment, comment: e.target.value });\r\n    };\r\n\r\n    // create comment\r\n\r\n    const onComment = (e) => {\r\n        e.preventDefault();\r\n        const postComments = async () => {\r\n            try {\r\n                await Axios.post(`/ap